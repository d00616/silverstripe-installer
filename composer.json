{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "self.version",
		"silverstripe/framework": "self.version",
		"silverstripe/reports": "self.version",
		"silverstripe/siteconfig": "self.version",
		"silverstripe-themes/simple": "3.1.*",
		"silverstripe/spamprotection": ">=1.2.1",
		"silverstripe/userforms": ">=3.1.0",
		"silverstripe/tagfield":  ">=1.2.1",
		"silverstripe/googlesitemaps": ">=1.4.0",
		"silverstripe/blog": ">=2.3.0"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7"
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
